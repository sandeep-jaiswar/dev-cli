{
  "name": "@sandeep-jaiswar/dev-cli",
  "version": "0.0.0",
  "description": "CLI for scaffolding internal React apps and components",
  "bin": {
    "dev": "./bin/index.js"
  },
  "scripts": {
    "build": "esbuild bin/index.ts --bundle --platform=node --outfile=bin/index.js --target=node18",
    "dev": "bun run bin/index.ts",
    "lint": "eslint . --ext .ts",
    "prepare": "bun run build"
  },
  "keywords": ["cli", "react", "scaffold", "devtools"],
  "license": "MIT",
  "dependencies": {
    "inquirer": "^9.0.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "@types/bun": "latest"
  },
  "type": "module",
  "peerDependencies": {
    "typescript": "^5"
  }
}
